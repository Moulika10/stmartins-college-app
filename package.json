{
  "name": "martins-api",
  "version": "1.0.0",
  "description": "API repository for services related to DTN Bid Publisher",
  "main": "dist/src/start-server.js",
  "scripts": {
    "start": "tsc && nodemon ./dist/src/start-server.js",
    "start:dev": "nodemon --inspect=5858 -e ts,tsx --exec node -r ts-node/register ./src/start-server.ts",
    "migrate-down": "npx sequelize-cli db:migrate:undo:all",
    "migrate-up": "npx sequelize-cli db:migrate",
    "seed-down": "npx sequelize-cli db:seed:undo:all",
    "seed-up": "npx sequelize-cli db:seed:all",
    "start-local": "npm run migrate-up && npm run seed-up && npm run start",
    "restart-db": "npm run seed-down && npm run migrate-down",
    "test": "npm run test:unit && npm run test:endpoint",
    "test:unit": "jest \"test/unit/.*\\.test\\.ts\"",
    "test:endpoint": "jest \"test/endpoint/.*\\.test\\.ts\"",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "coverage": "nyc -r lcov -e .ts -x \"*.ts\" npm run test",
    "docker:local": "sh scripts/docker-deploy-local.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.7.3",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-json-validator-middleware": "^2.1.1",
    "guid-typescript": "^1.0.9",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "redis": "^3.1.2",
    "sequelize": "^6.12.4",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.18.4",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.3",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.4"
  },
  "eslintIgnore": [
    "global.d.ts",
    "tests/**/*"
  ]
}
